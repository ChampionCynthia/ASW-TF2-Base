//================================================================================================
//
// IDLES
//
//================================================================================================

$sequence layer_idleNoise "primary_crouch_idle.dmx" loop subtract layer_idleNoise 0 iklock rfoot 1 0 iklock lfoot 1 0 delta hidden

$sequence Stand_PRIMARY  PRIMARY_idle frames 0 0 loop numframes 45 alignto a_reference addlayer PRIMARY_aimmatrix_idle addlayer layer_idleNoise activity ACT_MP_STAND_PRIMARY 1 
$sequence Stand_ITEM2  ITEM2_idle frames 0 0 loop numframes 45 alignto a_reference addlayer ITEM2_aimmatrix_idle addlayer layer_idleNoise activity ACT_MP_STAND_ITEM2 1 
$sequence Stand_SECONDARY  SECONDARY_idle frames 0 0 loop numframes 45 alignto a_reference addlayer SECONDARY_aimmatrix_idle addlayer layer_idleNoise activity ACT_MP_STAND_SECONDARY 1 
$sequence Stand_MELEE  MELEE_idle loop alignto a_reference addlayer MELEE_aimmatrix_idle activity ACT_MP_STAND_MELEE 1
$sequence Stand_STUN  MELEE_idle loop alignto a_reference addlayer STUN_aimmatrix_idle activity ACT_MP_STAND_MELEE 1
$sequence Stand_item1  item1_idle loop alignto a_reference addlayer item1_aimmatrix_idle activity ACT_MP_STAND_item1 1
$sequence Stand_LOSER  LOSER_idle loop alignto a_reference activity ACT_MP_STAND_LOSERSTATE 1


$sequence layer_null "primary_idle.dmx" noanimation hidden
//================================================================================================
//
// CROUCH IDLES
//
//================================================================================================

$sequence Crouch_PRIMARY PRIMARY_crouch_idle loop alignto a_reference addlayer PRIMARY_aimmatrix_idle  activity ACT_MP_CROUCH_PRIMARY 1
$sequence Crouch_ITEM2 ITEM2_crouch_idle loop alignto a_reference addlayer ITEM2_aimmatrix_idle  activity ACT_MP_CROUCH_ITEM2 1
$sequence Crouch_SECONDARY SECONDARY_crouch_idle loop alignto a_reference addlayer SECONDARY_aimmatrix_crouch_idle  activity ACT_MP_CROUCH_SECONDARY 1
$sequence Crouch_MELEE MELEE_crouch_idle loop  addlayer MELEE_aimmatrix_crouch_idle  activity ACT_MP_CROUCH_MELEE 1
$sequence Crouch_item1 item1_crouch_idle loop  addlayer item1_aimmatrix_crouch_idle  activity ACT_MP_CROUCH_item1 1
$sequence Crouch_LOSER LOSER_crouch_idle loop alignto a_reference activity ACT_MP_CROUCH_LOSERSTATE 1


//================================================================================================
//
// RUN PRIMARY
//
//================================================================================================

$animation a_runCenter_PRIMARY  primary_runCenter blockname $infoNode$ {
	loop
	alignto b_reference
}
$animation a_runS_PRIMARY primary_runs blockname $infoNode$ {
	LX LY
	rotateto -180
	startloop 9
	alignto a_reference
}
$animation a_runSE_PRIMARY primary_runse blockname $infoNode$ {
	LX LY
	rotateto -135
	startloop 9
	alignto a_reference
}
$animation a_runE_PRIMARY primary_rune blockname $infoNode$ {
	LX LY
	rotateto -90
	startloop 0
	alignto a_reference
}
$animation a_runNE_PRIMARY primary_runne blockname $infoNode$ {
	LX LY
	rotateto -45
	startloop 0
	alignto a_reference
}
$animation a_runN_PRIMARY primary_runn blockname $infoNode$ {
	LX LY
	rotateto 0
	startloop 0
	alignto a_reference
}
$animation a_runNW_PRIMARY primary_runnw blockname $infoNode$ {
	LX LY
	rotateto 45
	startloop 0
	alignto a_reference
}
$animation a_runW_PRIMARY primary_runw blockname $infoNode$ {
	LX LY
	rotateto 90
	startloop 0
	alignto a_reference
}
$animation a_runSW_PRIMARY primary_runsw blockname $infoNode$ {
	LX LY
	rotateto 135
	startloop 9
	alignto a_reference
}

//trying multi-direction aim blend (N S) using blendlayer
$MPAimLayer_leftHandIK primary_aimMatrix_runS primary_aimMatrix_runS $infoNode$
$sequence Run_PRIMARY {
	a_runsw_primary	a_runS_primary		a_runSE_primary
	a_runW_primary	a_runCenter_primary	a_runE_primary
	a_runNW_primary	a_runN_primary		a_runNE_primary
	blendlayer primary_aimmatrix_run -0.8 -0.2 1.0 2.0  spline poseparameter move_x 
  blendlayer primary_aimmatrix_runs -2.0 -1.0 -0.8 -0.2 spline poseparameter move_x 
	blendwidth 3 blend move_y -1 1 blend move_x -1 1
	activity ACT_MP_RUN_PRIMARY 1
	{ event 7001 1 "right" }
	{ event 7001 7 "left" }
	{ event 7001 13 "right" }
	{ event 7001 19 "left" }
	{ event 7001 25 "right" }
	{ event 7001 31 "left" }
	{ event 7001 37 "right" }
	{ event 7001 43 "left" }
}


